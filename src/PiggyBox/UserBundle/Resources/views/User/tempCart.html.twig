<script type="text/javascript">
(function ($) {
	$(document).ready(function() {
		$('select#piggybox_orderbundle_ordertype_pickupatDate_date').change(function (e) {
    	    var target = $(this);
			var shop_id =$('div.controls.data-load-shop');

    	    if (shop_id.attr('data-load-shop')) {
    	        $.ajax({
    	            url: Routing.generate('view_opening_hours', { "shop_id": shop_id.attr('data-load-shop'), "time_string" : target.val()}),
    	            dataType: 'json',
    	            success: function (data) {

						var select = $('select#piggybox_orderbundle_ordertype_pickupatTime_time');
						addOptionsToSelect(select, data.content);

						if(isEmpty(data.content)){
							var selectDate = document.getElementById("piggybox_orderbundle_ordertype_pickupatDate_date");
							selectDate.remove("0");
							console.log(selectDate.options[0].value);

							$.ajax({
								url: Routing.generate('view_opening_hours', { "shop_id": shop_id.attr('data-load-shop'), "time_string" : selectDate.options[0].value}),
								dataType: 'json',
								success: function (data) {
									var select = $('select#piggybox_orderbundle_ordertype_pickupatTime_time');
									addOptionsToSelect(select, data.content);
								}
							});
						}
					}
    	        });
    	    }
    	}).trigger('change');
	});

	function isEmpty(val){
    	return (val === undefined || val == null || val.length <= 0) ? true : false;
	}

	function addOptionsToSelect(select, content){
		if(select.prop) {
		  var options = select.prop('options');
		}

		else {
			var options = select.attr('options');
		}
		$('option', select).remove();
		select.append(content);
	}
})(jQuery);
</script>

         
