<h1>Order list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
			{% for orderdetail in entity.orderdetail%}
			<td>{{orderdetail.product.name}} + </td>
			{% endfor %}
        </tr>
    {% endfor %}
    </tbody>
</table>
